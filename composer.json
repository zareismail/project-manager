{
    "name": "zareismail/project-manager",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "zareismail/keil": "~0.1",
        "zareismail/task-management": "*",
        "armincms/nova-translation": "~1.1",
        "pishran/nova-rtl-theme": "^1.5",
        "maatwebsite/laravel-nova-excel": "~1.2",
        "zareismail/event-history": "*",
        "zareismail/board": "*",
        "zareismail/voice-recorder": "*",
        "zareismail/nova-resource-manager": "~0.2"
    },
    "autoload": {
        "psr-4": {
            "Zareismail\\ProjectManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zareismail\\ProjectManager\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
